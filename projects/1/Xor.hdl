/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=b, out=notb);
    Not(in=a, out=nota);
    And(a=a, b=notb, out=neq1);
    And(a=nota, b=b, out=neq2);
    Or(a=neq1, b=neq2, out=out);
}